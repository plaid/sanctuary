{
  "env": {
    "node": true
  },
  "globals": {},
  "rules": {
    "no-bitwise": 2,
    "eqeqeq": [2, "allow-null"],
    "wrap-iife": [2, "outside"],
    "no-caller": 2,
    "no-empty": 2,
    "no-new": 2,
    "no-plusplus": 2,
    "no-undef": 2,
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    "comma-style": [2, "last"],
    "dot-notation": 2,
  }
}
